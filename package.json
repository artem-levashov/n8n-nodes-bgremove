{
  "name": "n8n-nodes-bgremove",
  "version": "0.1.7",
  "description": "Remove background locally using transparent-background",
  "author": "kotlf<artkotlf@gmail.com>",
  "license": "MIT",
  "keywords": [
    "n8n",
    "image",
    "background removal",
    "transparent-background",
    "rembg"
  ],
  "main": "dist/RemoveBg.node.js",
  "types": "dist/RemoveBg.node.d.ts",
  "files": [
    "src",
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').cpSync('assets','dist/assets',{recursive:true})\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "transparent-background": "1.3.0",
    "jimp": "^0.22.10"
  },
  "devDependencies": {
    "n8n-core": "^1.47.0",
    "n8n-workflow": "^1.47.0",
    "typescript": "^5.4.0"
  },
  "n8n": {
    "nodes": [
      "dist/RemoveBg.node.js"
    ],
    "credentials": []
  }
}
